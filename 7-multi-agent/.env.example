# Google Cloud API Configuration
# Get your API key from: https://console.cloud.google.com/apis/credentials

# Vertex AI Configuration (set to FALSE to use standard Gemini API)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Main Google API Key (used for Gemini AI)
GOOGLE_API_KEY=your_google_api_key_here

# Google Places/Maps API Key (can be same as GOOGLE_API_KEY)
GOOGLE_PLACES_API_KEY=your_google_api_key_here

# OpenWeatherMap API Key (Optional - for weather features)
# Get free API key from: https://openweathermap.org/api
# Note: Weather feature works with fallback data if not provided
OPENWEATHER_API_KEY=your_openweather_api_key_here
